<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="treichel.screensharingmobile.ContactActivity">

    <TextView
        android:id="@+id/contactUsernameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="104dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="47dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/contactStatusTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/contactUsernameTextView"
        android:layout_alignRight="@+id/contactUsernameTextView"
        android:layout_below="@+id/contactUsernameTextView"
        android:layout_marginTop="20dp"
        android:text="TextView" />
</RelativeLayout>
